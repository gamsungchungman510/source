<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="daysstudio.com.toktoksurvey">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".setting.defaultset.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".setting.defaultset.RegisterActivity"/>
        <activity android:name=".setting.SurveyList.SurveyList"/>
        <activity android:name=".setting.DirectSurvey.ParticipateSurvey.Participate_survey"/>
        <activity android:name=".setting.DirectSurvey.MakeSurvey.MakeList"/>
        <activity android:name=".setting.SurveyList.po"/>
    </application>

</manifest>